{
  "name": "restapp",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": ">= 6.11.1 <= 10",
    "yarn": ">= 1.0.0"
  },
  "cacheDirectories": [
    ".cache"
  ],
  "scripts": {
    "build": "lerna run build --stream",
    "clean": "lerna run clean --stream",
    "start": "lerna run --scope=server start --stream",
    "watch": "zen watch",
    "watch:android": "zen -e android -d client -d ios watch",
    "watch:ios": "zen -e ios -d client -d android watch",
    "watch-client": "cross-env DISABLE_SSR=true lerna run --scope=client watch --stream",
    "cli": "node tools/cli",
    "seed": "lerna run --scope=server seed --stream",
    "migrate": "lerna run --scope=server migrate",
    "rollback": "lerna run --scope=server rollback",
    "exp": "yarn --cwd packages/mobile exp",
    "exp-publish": "lerna run --scope=mobile exp-publish --stream",
    "tests": "jest",
    "tests:watch": "jest --watch",
    "lint": "yarn eslint && yarn tslint",
    "eslint": "eslint --fix --ext js --ext jsx --ext json .",
    "tslint": "tslint --fix -p tsconfig.json -c tslint.json",
    "heroku-postbuild": "lerna run heroku-postbuild --stream",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "eslint --fix",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sysgears/restapp"
  },
  "bugs": {
    "url": "https://github.com/sysgears/restapp/issues"
  },
  "homepage": "https://github.com/sysgears/restapp",
  "keywords": [
    "restapp",
    "rest",
    "react",
    "react-native",
    "redux"
  ],
  "author": "SysGears INC",
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "modules/**/client-react",
    "modules/**/client-react-native",
    "modules/**/common",
    "modules/**/common-react",
    "modules/**/server-ts"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-destructuring": "^7.0.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@larix/zen": "^0.1.9",
    "autoprefixer": "^9.5.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-styled-components": "^1.3.0",
    "babel-preset-minify": "^0.5.0-alpha.5a128fd5",
    "cache-loader": "^1.2.2",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^1.0.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-react-hooks": "^1.0.1",
    "file-loader": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^0.4.9",
    "heroku-babel-loader": "^8.0.4",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.0",
    "husky": "^1.1.2",
    "ignore-loader": "^0.1.2",
    "isomorphic-style-loader": "^4.0.0",
    "lerna": "^3.4.0",
    "less": "^3.0.2",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.1.2",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.7.2",
    "openurl": "^1.1.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.9.2",
    "qrcode-terminal": "^0.12.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.1",
    "source-list-map": "^2.0.0",
    "style-loader": "^0.23.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.5.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.2.4",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.0.1",
    "wait-on": "^3.0.1",
    "webpack": "^4.29.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.5",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2",
    "webpack-sources": "^1.1.0",
    "webpack-virtual-modules": "^0.1.10",
    "whatwg-fetch": "^2.0.4"
  },
  "dependencies": {
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "jest-transform-stub": "^2.0.0",
    "lodash": "^4.17.11",
    "ts-jest": "^24.0.2"
  },
  "resolutions": {
    "immutability-helper": "2.8.1",
    "iterall": "^1.2.2",
    "upath": "^1.1.0",
    "watchpack": "https://github.com/Globegitter/watchpack"
  }
}
