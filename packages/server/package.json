{
  "name": "server",
  "private": true,
  "version": "0.1.0",
  "main": "src",
  "engines": {
    "node": ">= 6.11.1 <= 10",
    "yarn": ">= 1.0.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production zen build",
    "clean": "rimraf build .tmp",
    "start": "node --harmony build",
    "tests": "cross-env NODE_ENV=test PORT=7070 zen test -t 10000 --include src/__tests__/entry.ts --full-trace \"src/**/*.spec.*\" \"../../modules/**/server-ts/**/*.spec.*\"",
    "tests:watch": "cross-env NODE_ENV=test PORT=7070 zen test -t 10000 --include src/__tests__/entry.ts --full-trace --watch \"src/**/*.spec.*\" \"../../modules/**/server-ts/**/*.spec.*\"",
    "test": "yarn tests && yarn lint",
    "eslint": "eslint --fix --ext js --ext jsx --ext json src",
    "tslint": "tslint --fix -p tsconfig.json -c ../../tslint.json",
    "lint": "yarn eslint && yarn tslint",
    "seed": "yarn migrate && knex seed:run",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "watch": "zen watch",
    "heroku-postbuild": "yarn build && yarn seed",
    "cli": "node ../../tools/cli"
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
